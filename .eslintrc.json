{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },

  "rules": {
    "indent": ["error", 2],
    "quotes": ["error", "double"],
    "linebreak-style": 0,
    "object-curly-newline": ["error", { "multiline": true }],
    "comma-dangle": ["error", "never"],
    "no-console": 0
  }
}
